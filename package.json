{
  "name": "note-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && prettier --check \"app/**/*.{ts,tsx}\"",
    "db:gen": "dotenv -e .env.development -- drizzle-kit generate:pg",
    "db:push": "dotenv -e .env.development -- drizzle-kit push:pg",
    "db:pull": "dotenv -e .env.development -- drizzle-kit introspect:pg",
    "db:seed": "dotenv -e .env.development -- ts-node db/seed.ts",
    "db:migrate": "dotenv -e .env.development -- ts-node db/migrate.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@tabler/icons-react": "^2.23.0",
    "@vercel/postgres": "^0.4.0",
    "dayjs": "^1.11.8",
    "drizzle-orm": "^0.27.0",
    "next": "13.4.7",
    "next-auth": "^4.22.1",
    "next-intl": "3.0.0-beta.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/eslint": "^8.40.2",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "10.4.14",
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.19.2",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  }
}
